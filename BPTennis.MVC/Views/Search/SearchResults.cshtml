@model BPTennis.MVC.Models.SearchResultModel

@{
    ViewBag.Title = "SearchResults";
}

<h2>Search Results</h2>

<table>
    <tr>
        <th>
            Player
        </th>
        <th>
            Games Played
        </th>
    </tr>
@foreach (var item in Model.PlayerGames) {
    <tr>
        <td>
            @item.Name
        </td>
        <td>
            @item.NumberOfGames
        </td>

    </tr>
}
    <tr>
        <td>
           | @Html.ActionLink("Back", "Index") |
        </td>
    </tr>
</table>
