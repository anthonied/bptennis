}

<h2>Index</h2>

<p>
    @Html.ActionLink("Add Player", "Add")
</p>
<table>
    <tr>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
